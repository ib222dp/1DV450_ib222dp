<h1>Admin - API-nycklar</h1>

<% if !@keys.nil? %>
  <ul>
    <% @keys.each do |key| %>
      <li><%= link_to "#{key.api_key_value}", { :controller => "admin_api_keys", :action => "revokekey", :apikey => key.id}  %></li>
    <% end %>
  </ul>
<% else %>
  <p>Inga API-nycklar att visa.</p>
<% end %>

<p><%= link_to "Logga ut", adminlogout_path %></p>